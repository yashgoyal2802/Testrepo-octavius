                + '","sTelOffTwo":"' + $('#txtTelOffTwo').val()
                 + '","sFax":"' + $('#txtFax').val()
                  + '","sEmailID":"' + $('#txtEmail').val() + '"'
        + "}";


            $.ajax({
                type: "POST",
                url: '@Url.Action("EditAccountDetails")',
                data: jsonStringVal,
                contentType: "application/json; charset=utf-8",
                dataType: "json",
                success: function (response) {
                    document.getElementById('lightEdit').style.display = 'none';
