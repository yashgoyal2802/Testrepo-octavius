                    location.reload();
                    //fnShowMessageBox("Rank Weightage Management", "<br/>Default Rank Attribute Weightages have been set successfully!<br /> <br/> Expert Portal Team", 1, function abc() { }, null);
                },
                error: function (error) {
                    alert('error; ' + eval(error));
                },
            });
        }
