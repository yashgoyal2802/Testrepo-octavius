        function saveAccountDetails(obj) {
            //msg = "Are you sure you want to set default Ranking Weightage such as : <br/><ui><li> Publications Weightage : 34 %</li> <li> Clinical Trials : 33 %</li> <li> Conferences / Podium Speaker : 33 %</li></ui>"

